{
  "name": "moyu",
  "version": "0.6.0",
  "description": "eggjs egg tool moyu",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "ali-oss": "^6.5.1",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.0",
    "egg-mongoose": "^3.2.0",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.2",
    "escape-string-regexp": "^4.0.0",
    "file-type": "^14.6.2",
    "fs-extra": "^9.0.0",
    "officegen": "^0.6.4",
    "svg-captcha": "^1.4.0",
    "xlsx": "^0.16.5",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^7.27.0",
    "eslint-config-egg": "^9.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^6.0.0",
    "sequelize-cli": "^6.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix .",
      "pre-push": "eslint --fix ."
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-too",
    "stop": "egg-scripts stop --title=egg-server-too",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trueleaf/moyu-server"
  },
  "author": "shuxiaokai",
  "license": "MIT"
}
